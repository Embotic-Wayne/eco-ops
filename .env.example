# LLM: use either Groq OR GreenPT (sponsor track)
# Groq API Key (default)
# Get your API key from https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here

# GreenPT API (sponsor track) â€“ when set, all agents use GreenPT instead of Groq
# Get your API key from https://greenpt.ai/ (API-only plans available)
# GREENPT_API_KEY=your_greenpt_api_key_here
# GREENPT_BASE_URL=https://api.greenpt.ai/v1
# GREENPT_MODEL=green-l

# Mapbox Access Token
# Get your token from https://account.mapbox.com/access-tokens/
# Create a token and add it here (starts with pk.)
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here

# Fish Audio (Text-to-Speech)
# Get your API key from https://fish.audio/
FISH_AUDIO_API_KEY=your_fish_audio_api_key_here

# Optional: choose a specific voice/model from Fish Audio Playground/Discovery
# Example reference_id from docs: 7f92f8afb8ec43bf81429cc1c9199cb1
FISH_AUDIO_REFERENCE_ID=

# Optional: model header (defaults to s1)
FISH_AUDIO_MODEL=s1
